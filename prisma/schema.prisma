// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Cliente {
  id    Int     @id @default(autoincrement())
  nome String  @unique
  email  String
  senha String
}

model Produto {
  id    Int     @id @default(autoincrement())
  nome String  @unique
  quantidade  Int
  preco String
}

model Contas {
  id    Int     @id @default(autoincrement())
  nome String  @unique
  descricao  String
  vencimento String
  status  Boolean    @default(false)
  valor Float
}
